/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in = sel, out = notOut);
    And(a = a, b = notOut, out = outA);
    And(a = b, b = sel, out = outB);
    Or(a = outA, b = outB, out = out);
}